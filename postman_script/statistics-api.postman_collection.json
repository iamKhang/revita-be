      "name": "1. Quick KPI",
      "item": [
        {
          "name": "Get Quick KPI - Today",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{access_token}}",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{base_url}}/statistics/kpi?period=day",
              "host": ["{{base_url}}"],
              "path": ["statistics", "kpi"],
              "query": [
                {
                  "key": "period",
                  "value": "day"
                }
              ]
            }
          }
        },
        {
